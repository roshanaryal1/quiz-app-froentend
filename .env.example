# Environment Variables Example
# Copy this file to .env to use custom settings

# API Configuration - Manual Override
# Uncomment and set to force a specific backend URL
# VITE_API_BASE_URL=http://localhost:8080/api
# VITE_API_BASE_URL=https://quiz-tournament-api.onrender.com/api

# Development Settings
# VITE_DEBUG_MODE=true
# VITE_API_LOGGING=true
# VITE_CACHE_DISABLED=true

# Local Backend Settings
# VITE_LOCAL_API_PORT=8080
# VITE_LOCAL_API_HOST=localhost

# Environment Mode
# VITE_APP_ENV=development
